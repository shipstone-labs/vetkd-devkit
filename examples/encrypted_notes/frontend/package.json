    "prettier": "3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "rollup-plugin-css-only": "^4.5.2",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.26.1",
    "vite": "^5.4.14",
    "vite-plugin-compression": "^0.5.1",
